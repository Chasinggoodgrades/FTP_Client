<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="526.0" prefWidth="697.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxmlControllers.FTPMain">
   <children>
      <VBox prefHeight="526.0" prefWidth="697.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane prefHeight="27.0" prefWidth="697.0">
               <children>
                  <MenuBar fx:id="mainMenuBar" prefHeight="28.0" prefWidth="627.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="70.0">
                    <menus>
                      <Menu fx:id="menuFile" mnemonicParsing="false" text="File">
                        <items>
                              <MenuItem fx:id="fileAdmin" mnemonicParsing="false" onAction="#handleFileAdmin" text="Admin" />
                              <MenuItem fx:id="fileLogout" mnemonicParsing="false" onAction="#handleFileLogout" text="Logout" />
                        </items>
                      </Menu>
                    </menus>
                  </MenuBar>
                  <Button fx:id="uploadButton" layoutX="627.0" mnemonicParsing="false" prefHeight="28.0" prefWidth="70.0" text="Upload" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
