                        <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TreeTableView>
               </children></AnchorPane>
            <AnchorPane prefHeight="52.0" prefWidth="697.0">
               <children>
                  <ToolBar prefHeight="52.0" prefWidth="529.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="168.0">
