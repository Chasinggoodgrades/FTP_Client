                      <Button fx:id="downloadButton" mnemonicParsing="false" text="Download" />
                    </items>
                  </ToolBar>
                  <ToolBar layoutX="257.0" prefHeight="52.0" prefWidth="170.0" AnchorPane.rightAnchor="0.0">
                     <items>
                        <Button fx:id="shareButton" mnemonicParsing="false" onAction="#handleShareButton" prefHeight="28.0" prefWidth="70.0" text="Share" />
                        <Button fx:id="deleteButton" mnemonicParsing="false" prefHeight="28.0" prefWidth="76.0" text="Delete" />
                     </items>
                  </ToolBar>
               </children></AnchorPane>
         </children>
      </VBox>
   </children>
